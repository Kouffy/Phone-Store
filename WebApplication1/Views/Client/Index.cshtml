
@{
    ViewBag.Title = "Index";
}
@{ if (Session["iduser"] == null)
    {
        Response.Redirect("/Client/Login");
    }
    else if (Session["previlege"].ToString() != "admin")
    {
        Response.Redirect("/Home/Indix");
    }
}
<!DOCTYPE html>
<html>
<head>
