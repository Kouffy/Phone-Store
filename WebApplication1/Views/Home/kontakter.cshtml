    <body>
        <!--header-->
        <div class="header header-main">
            <div class="container">
                <div class="header-top">
                    <div class="logo">
                        <h1><a href="index.html">Phone Store</a></h1>
                    </div>
                    <div class="top-nav">
                        <span class="menu"><img src="~/images/menu.png" alt=""> </span>
                        <ul>
                            <li><a href="/Home/acoy" class="hvr-sweep-to-bottom color"><i class="glyphicon glyphicon-home"></i>Home</a> </li>
                            <li><a href="/Home/Indix" class="hvr-sweep-to-bottom color1"><i class="glyphicon glyphicon-picture"></i>Produits</a> </li>
                            <li><a href="/Home/kontakter" class="hvr-sweep-to-bottom color2"><i class="glyphicon glyphicon-tags"></i>Contacter</a></li>
                            <li><a href="/Client/Logout" class="btn btn-danger"><i class="glyphicon glyphicon-remove"></i>Deconnection</a></li>
                            <div class="clearfix"> </div>
                        </ul>
                        <!--script-->
                        <script>
                            $("span.menu").click(function () {
                                $(".top-nav ul").slideToggle(500, function () {
                                });
                            });
                        </script>
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <!---->
            </div>
        </div>
        <!--//header-->
        <!--content-->
        <div class="contact" id="message">
            <div class="container">
                <div class="contact-top ">
                    <h3>Contact Us</h3>
                </div>

                <div class="contact-grids">
                    <div class="contact-form">
                        <form v-on:submit.prevent="addMsg()">
                            <div class="contact-bottom">
                                <div class="col-md-4 in-contact">
                                    <span>id :</span>
                                    <input type="text" class="text" value="" v-model="ObjMsg.id" disabled>
                                </div>
                                <div class="col-md-4 in-contact">
                                    <span>Name :</span>
                                    <input type="text" class="text" value="" v-model="ObjMsg.name">
                                </div>
                                <div class="col-md-4 in-contact">
                                    <span>EMail :</span>
                                    <input type="text" class="text" value="" v-model="ObjMsg.email">
                                </div>
                                <div class="col-md-4 in-contact">
                                    <span>Subject :</span>
                                    <input type="text" class="text" value="" v-model="ObjMsg.sujet">
                                </div>
                                <div class="clearfix"> </div>
                            </div>
                            <div class="clearfix"> </div>
                            <div class="contact-bottom-top">
                                <span>Message :</span>
                                <textarea v-model="ObjMsg.MESSAGE"> </textarea>
                            </div>
                            <input type="submit" value="Send">
                        </form>
                    </div>
           <br /><br /><hr /><br />
        <!--//content-->
        <!--footer-->
        <div class="footer">
            <div class="container">
                <h2><a href="index.html">Phone Store</a></h2>

                <ul>
                    <li><a href="/Home/acoy">Home  </a> </li>
                    <li><a href="/Home/indix">Produits</a> </li>
                    <li><a href="/Home/kontakter">Contacter</a></li>
                    <div class="clearfix"> </div>
                </ul>
                <p> © 2019 Phone Store All rights reserved </p>
            </div>
        </div>
        <!--//footer-->
    </body>
</html>
